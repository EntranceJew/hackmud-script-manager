{
	"name": "hackmud-script-manager",
	"version": "0.18.0",
	"description": "Script manager for game hackmud, with minification, TypeScript support, and player script type definition generation.",
	"keywords": [
		"api",
		"command-line",
		"script",
		"manager",
		"minification",
		"minifier",
		"hackmd",
		"hackmud",
		"javascipt",
		"typescipt",
		"typescript-support",
		"golf",
		"golfer"
	],
	"homepage": "https://github.com/samualtnorman/hackmud-script-manager#readme",
	"bugs": "https://github.com/samualtnorman/hackmud-script-manager/issues",
	"license": "GPL-3.0-or-later",
	"author": "Samual Norman",
	"main": "index.js",
	"repository": {
		"type": "git",
		"url": "https://github.com/samualtnorman/hackmud-script-manager.git"
	},
	"scripts": {
		"build": "rollup --config",
		"dev": "rollup --config --watch",
		"lint": "eslint .; tsc --noEmit --emitDeclarationOnly false; tsc --project src --noEmit --emitDeclarationOnly false",
		"test": "node --experimental-vm-modules node_modules/jest/bin/jest",
		"generate-type-declarations": "tsc --project src",
		"package": "rm -rf dist && rollup --config && tsc --project src && node scripts/build-package-json && cp LICENSE README.md dist"
	},
	"dependencies": {
		"@babel/core": "^7.18.9",
		"@babel/generator": "^7.18.9",
		"@babel/parser": "^7.18.9",
		"@babel/plugin-proposal-class-properties": "^7.18.6",
		"@babel/plugin-proposal-class-static-block": "^7.18.6",
		"@babel/plugin-proposal-decorators": "^7.18.9",
		"@babel/plugin-proposal-do-expressions": "^7.18.6",
		"@babel/plugin-proposal-function-bind": "^7.18.9",
		"@babel/plugin-proposal-function-sent": "^7.18.6",
		"@babel/plugin-proposal-json-strings": "^7.18.6",
		"@babel/plugin-proposal-logical-assignment-operators": "^7.18.9",
		"@babel/plugin-proposal-nullish-coalescing-operator": "^7.18.6",
		"@babel/plugin-proposal-numeric-separator": "^7.18.6",
		"@babel/plugin-proposal-object-rest-spread": "^7.18.9",
		"@babel/plugin-proposal-optional-catch-binding": "^7.18.6",
		"@babel/plugin-proposal-optional-chaining": "^7.18.9",
		"@babel/plugin-proposal-partial-application": "^7.18.9",
		"@babel/plugin-proposal-pipeline-operator": "^7.18.9",
		"@babel/plugin-proposal-private-property-in-object": "^7.18.6",
		"@babel/plugin-proposal-record-and-tuple": "^7.18.6",
		"@babel/plugin-proposal-throw-expressions": "^7.18.6",
		"@babel/plugin-transform-exponentiation-operator": "^7.18.6",
		"@babel/plugin-transform-typescript": "^7.18.8",
		"@babel/traverse": "^7.18.9",
		"@babel/types": "^7.18.9",
		"@bloomberg/record-tuple-polyfill": "^0.0.4",
		"@rollup/plugin-babel": "^5.3.1",
		"@rollup/plugin-commonjs": "^22.0.1",
		"@rollup/plugin-json": "^4.1.0",
		"@rollup/plugin-node-resolve": "^13.3.0",
		"acorn": "^8.7.1",
		"chalk": "^5.0.1",
		"chokidar": "^3.5.3",
		"import-meta-resolve": "^2.0.3",
		"prettier": "^2.7.1",
		"proxy-polyfill": "^0.3.2",
		"rollup": "^2.77.0",
		"terser": "^5.14.2"
	},
	"devDependencies": {
		"@babel/preset-env": "^7.18.9",
		"@babel/preset-typescript": "^7.18.6",
		"@samual/lib": "^0.3.4",
		"@types/babel__core": "^7.1.19",
		"@types/babel__generator": "^7.6.4",
		"@types/babel__traverse": "^7.17.1",
		"@types/node": "^14.18.22",
		"@types/prettier": "^2.6.3",
		"@types/semver": "^7.3.10",
		"@typescript-eslint/eslint-plugin": "^5.30.7",
		"@typescript-eslint/parser": "^5.30.7",
		"eslint": "^8.20.0",
		"eslint-plugin-array-func": "^3.1.7",
		"eslint-plugin-eslint-comments": "^3.2.0",
		"eslint-plugin-regexp": "^1.7.0",
		"eslint-plugin-unicorn": "^43.0.2",
		"latest-version": "^7.0.0",
		"rollup-plugin-preserve-shebang": "^1.0.1",
		"rollup-plugin-terser": "^7.0.2",
		"semver": "^7.3.7",
		"typescript": "^4.7.4"
	},
	"optionalDependencies": {
		"deasync": "^0.1.27"
	},
	"engines": {
		"node": ">=14"
	},
	"private": true,
	"types": "index.d.ts",
	"type": "module",
	"exports": {
		".": {
			"import": "./index.js",
			"require": "./index.cjs"
		},
		"./*": "./*"
	}
}
